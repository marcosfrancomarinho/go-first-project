{
  "devDependencies": {
    "concurrently": "^9.2.0"
  },
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "install": "concurrently \"npm run install:frontend\" \"npm run install:backend\"",
    "start:frontend": "cd ./frontend && yarn dev",
    "start:backend": "cd ./backend && air",
    "install:frontend": "cd ./frontend && yarn install",
    "install:backend": "cd ./backend && go mod tidy"
  }
}
